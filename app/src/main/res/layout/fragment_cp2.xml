            <Button
                android:id="@+id/btcp2_21"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="21"
                android:textSize="30sp"
                />

            <Button
                android:id="@+id/btcp2_22"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="22"
                android:textSize="30sp"
                />

            <Button
                android:id="@+id/btcp2_23"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
